{
  "version": 2,
  "builds": [
    { "src": "vite.config.ts", "use": "@vercel/static-build" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "VITE_API_URL": "https://cryptochat-backend.onrender.com"
  },
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
